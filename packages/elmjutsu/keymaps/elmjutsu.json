{
  "atom-text-editor.elmjutsu-rename-symbol": {
    "enter": "elmjutsu:confirm-rename-symbol",
    "escape": "elmjutsu:cancel-rename-symbol"
  },
  ".elmjutsu-pipe-selections": {
    "ctrl-enter": "elmjutsu:apply-pipe-selections",
    "cmd-enter": "elmjutsu:apply-pipe-selections",
    "escape": "elmjutsu:cancel-pipe-selections"
  },
  "atom-text-editor.elmjutsu-set-main-paths": {
    "escape": "elmjutsu:cancel-set-main-paths",
    "enter": "elmjutsu:confirm-set-main-paths"
  },
  "atom-text-editor.elmjutsu-set-compile-output-path": {
    "escape": "elmjutsu:cancel-set-compile-output-path",
    "enter": "elmjutsu:confirm-set-compile-output-path"
  }
}
